{
  "name": "react-fullpage-accordion",
  "version": "2.0.3",
  "description": "Full page accordion react component",
  "main": "dist/index.js",
  "author": "Brian Andrews <sbardian@gmail.com> (https://github.com/sbardian)",
  "license": "MIT",
  "scripts": {
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --coverageReporters=text-lcov | coveralls",
    "lint": "eslint --ext \".js,.ts,.tsx\" .",
    "devBuild": "babel-watch src/index.tsx",
    "build": "babel src --out-dir dist --ignore \"**/*.test.js\" --extensions '.tsx' --copy-files && rm dist/*.test.js",
    "prepublishOnly": "yarn check-types && yarn lint && yarn build",
    "check-types": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sbardian/react-fullpage-accordion"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "normalize.css": "8.0.1",
    "prop-types": "15.7.2",
    "react": "16.10.2",
    "react-dom": "16.10.2"
  },
  "devDependencies": {
    "@babel/cli": "7.6.4",
    "@babel/core": "7.6.4",
    "@babel/plugin-proposal-class-properties": "7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "7.6.2",
    "@babel/plugin-transform-arrow-functions": "7.2.0",
    "@babel/preset-env": "7.6.3",
    "@babel/preset-react": "7.6.3",
    "@babel/preset-typescript": "7.6.0",
    "@testing-library/react": "9.3.0",
    "@types/jest": "24.0.19",
    "@types/react": "16.9.9",
    "@types/react-dom": "16.9.2",
    "@typescript-eslint/eslint-plugin": "2.5.0",
    "@typescript-eslint/parser": "2.5.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.3",
    "babel-jest": "24.9.0",
    "babel-loader": "8.0.6",
    "coveralls": "3.0.7",
    "eslint": "6.5.1",
    "eslint-config-airbnb": "18.0.1",
    "eslint-config-prettier": "6.4.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jest": "22.19.0",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.16.0",
    "jest": "24.9.0",
    "prettier": "1.18.2",
    "regenerator-runtime": "0.13.3",
    "ts-jest": "24.1.0",
    "typescript": "3.6.4"
  }
