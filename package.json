    "@types/testing-library__dom": "^6.10.0",
    "@types/testing-library__react": "^9.1.2",
    "normalize.css": "8.0.1",
    "prop-types": "15.7.2",
    "react": "16.11.0",
    "react-dom": "16.11.0"
  },
  "devDependencies": {
    "@babel/cli": "7.7.0",
    "@babel/core": "7.7.0",
    "@babel/plugin-proposal-class-properties": "7.7.0",
